{
  "name": "__MSG_ExtensionName__",
  "description": "__MSG_ExtensionDescription__",
  "version": "1.2",
  "author": "SONN Patentanw√§lte GmbH & Co KG (dpf)",
  "applications": {
    "gecko": {
      "id": "ToDoCompanion@SONN",
      "strict_min_version": "115.0"
    }
  },
  "default_locale": "en",
  "permissions": [
    "messagesRead",
    "accountsRead",
    "messagesMove",
    "activeTab",
    "tabs",
    "notifications",
    "storage"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "message_display_action": {
    "default_title": "__MSG_ExtensionActionTitle__",
    "default_icon": "images/logo.svg",
    "default_label": "__MSG_ExtensionActionLabel__"
  },
  "experiment_apis": {
    "ImapTools": {
      "schema": "api/imapTools/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "api/imapTools/script.js",
        "paths": [
          [
            "ImapTools"
          ]
        ]
      }
    }
  },
  "manifest_version": 2,
  "icons": {
    "16": "images/logo.svg",
    "32": "images/logo.svg",
    "48": "images/logo.svg",
    "96": "images/logo.svg"
  }
}